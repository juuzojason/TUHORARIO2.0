<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox minHeight="-Infinity" minWidth="-Infinity" prefHeight="240.0" prefWidth="560.0" spacing="10.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.tuhorario2.Controllers.Admin.OptionEditController">
   <HBox prefHeight="60.0" prefWidth="200.0">
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="180.0">
         <TextField fx:id="AddLabelTextField" prefHeight="30.0" prefWidth="118.0" promptText="Add label" style="-fx-border-color: #ffffff;" />
         <Button fx:id="AddLabelButton" mnemonicParsing="false" text="+" />
      </HBox>
      <FlowPane fx:id="LabelFlowPane" hgap="10.0" prefHeight="100.0" prefWidth="382.0" vgap="10.0">
         <HBox.margin>
            <Insets />
         </HBox.margin>
         <padding>
            <Insets bottom="10.0" right="10.0" top="10.0" />
         </padding>
      </FlowPane>
   </HBox>
   <VBox prefHeight="180.0" prefWidth="100.0">
      <HBox alignment="CENTER" prefHeight="30.0" prefWidth="600.0" spacing="80.0">
         <Label text="Day" />
         <Label text="Begin Hour" />
         <Label text="End Hour" />
         <Label text="Delete" />
      </HBox>
      <VBox fx:id="DayVBox" alignment="TOP_CENTER" prefHeight="160.0" prefWidth="100.0">
         <Button fx:id="AddDayButton" mnemonicParsing="false" onAction="#AddDayLister" text="+" />
      </VBox>
   </VBox>
</VBox>

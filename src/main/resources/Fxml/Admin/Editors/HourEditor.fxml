<DialogPane maxHeight="130.0" maxWidth="250.0" prefHeight="87.0" prefWidth="250.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.tuhorario2.Controllers.Admin.HourEditorController">
   <header>
      <HBox alignment="TOP_RIGHT" prefHeight="30.0" prefWidth="250.0" style="-fx-background-color: #303030; -fx-background-radius: 15 15 0 0;">
         <AnchorPane minHeight="-Infinity" prefHeight="30.0" prefWidth="250.0">
            <Label alignment="CENTER" layoutX="41.0" layoutY="6.0" prefHeight="17.0" prefWidth="154.0" text="Edit Hour" textFill="WHITE" />
         </AnchorPane>
      </HBox>
   </header>
   <content>
      <VBox minHeight="-Infinity" minWidth="-Infinity" prefHeight="80.0" prefWidth="250.0">
         <HBox alignment="CENTER" minHeight="-Infinity" minWidth="-Infinity" prefHeight="40.0" prefWidth="233.0">
            <Spinner fx:id="HoursSpinner" editable="true" prefHeight="25.0" prefWidth="87.0" />
            <ChoiceBox fx:id="MinuteChoiceBox" prefHeight="25.0" prefWidth="87.0" />
